{
  "metadata": [
    {
      "src": [
        {
          "files": [ "src/Unosquare.RaspberryIO/**/*.cs" ],
          "exclude": [ "**/bin/**", "**/obj/**" ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "cwd": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "*.md", "toc.yml" ]
      }
    ],
    "resource": [
      {
        "files": [ "logos/raspberryio-logo-32.png" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "RaspberryIO - Pi's hardware access from Mono",
      "_enableSearch": true,
      "_appLogoPath": "logos/raspberryio-logo-32.png"
    },
    "dest": "_site"
  }
}
